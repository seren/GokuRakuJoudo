<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>$PLIST-NAME$</string>
  <key>ProgramArguments</key>
  <array>
    <string>$OPT-BIN-PATH$/goku</string>
  </array>
  <key>WatchPaths</key>
  <array>
      <string>$HOME-PATH$/.config/karabiner.edn</string>
  </array>
  <key>EnvironmentVariables</key>
  <dict>
      <key>GOKU_EDN_CONFIG_FILE</key>
      <string>$HOME-PATH$/.config/karabiner.edn</string>
  </dict>
  <key>StandardErrorPath</key>
  <string>$HOME-PATH$/Library/Logs/goku.log</string>
  <key>StandardOutPath</key>
  <string>$HOME-PATH$/Library/Logs/goku.log</string>
  <key>RunAtLoad</key>
  <true/>
</dict>
</plist>
